{
  "compilerOptions": {
    "target": "ES6",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "./dist",
    "rootDir": "./",
    "sourceMap": true,
    "declaration": true,
    "declarationMap": true,
    "moduleResolution": "node"
  },
  "include": ["server.ts"],
  "exclude": ["node_modules", "interfaces"]
}
